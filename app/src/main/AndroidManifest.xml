<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.ireader">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
      
        <activity android:name=".c_xiugaiActivity"></activity>
        <activity android:name=".c_xggrActivity" />
        <activity android:name=".c_grActivity" />
        <activity android:name=".c_configActivity" />
        <activity android:name=".c_titleActivity" />
        <activity android:name=".c_registerActivity" />
        <activity android:name=".c_loginActivity" />
        <activity android:name=".c_indexActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".g_resou3" />
        <activity android:name=".g_resou2" />
        <activity android:name=".g_fenlei"></activity>
        <activity android:name=".g_feilei" />
        <activity android:name=".g_ziye" />
        <activity android:name=".g_resou" />
        <activity android:name=".g_grid" />
        <activity android:name=".g_item" />
        <activity android:name=".g_custom"/>
        <activity android:name=".g_search"/>
        <activity android:name=".g_leibiao"/>
        <!--首页-->
        <activity
            android:name=".w_main_pageActivity"
            android:label="@string/title_activity_w_main_page">

        </activity>

        <activity android:name=".w_list_Activity"></activity>
        <activity android:name=".w_open_local_file_Activity" >
        </activity>
        <activity
            android:name="com.bifan.txtreaderlib.ui.HwTxtPlayActivity"
            android:theme="@style/HwTxtFullScreen">
            <intent-filter>
                <data android:scheme="txt" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data android:mimeType="text/plain" />
            </intent-filter>
        </activity>
        <activity
            android:name=".w_detailActivity"
            android:label="@string/title_activity_w_detail"
            android:theme="@style/AppTheme.NoActionBar" >

        </activity>

        <activity android:name=".MainActivity" />

    </application>

</manifest>